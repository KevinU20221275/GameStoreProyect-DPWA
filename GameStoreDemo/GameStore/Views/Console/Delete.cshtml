@model MConsole
@{
	ViewData["title"] = "Eliminar Consola";
}

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <dl>

                <dt>@Html.DisplayNameFor(model => model.ConsoleName)</dt>
                <dd>@Html.DisplayFor(model => model.ConsoleName)</dd>

                <dt>@Html.DisplayNameFor(model => model.Description)</dt>
                <dd>@Html.DisplayFor(model => model.Description)</dd>
            </dl>
            <form method="post" asp-action="Delete" class="mt-3">
                <input asp-for="idConsole" hidden/>
                <input type="submit" value="Eliminar" class="btn btn-danger" />

                <a asp-action="Index" class="btn btn-primary">Volver al Listado</a>
            </form>
        </div>
    </div>
</div>
